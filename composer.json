{
    "name": "jimmyhowedotcom/singlesignon",
    "description": "Single Sign-On Client for JimmyHowe.com",
    "license": "JimmyHowe.com 2021",
    "authors": [
        {
            "name": "Jimmy Howe",
            "email": "director@jimmyhowe.com",
            "homepage": "https://jimmyhowe.com"
        }
    ],
    "homepage": "https://github.com/jimmyhowedotcom/singlesignon",
    "keywords": [
        "Laravel",
        "SingleSignOn"
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "JimmyHoweDotCom\\SingleSignOn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JimmyHoweDotCom\\SingleSignOn\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JimmyHoweDotCom\\SingleSignOn\\SingleSignOnServiceProvider"
            ],
            "aliases": {
                "SingleSignOn": "JimmyHoweDotCom\\SingleSignOn\\Facades\\SingleSignOn"
            }
        }
    }
}
